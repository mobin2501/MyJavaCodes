package eularProject;

public class NonAbundantSums {
	public static void main(String args[]) {
		for(int i=1;i<28123;i++) {
			int sum=0;
			for(int j=1;j<i;j++) {
				System.out.print(i+" =");
				if(i%j==0) {
					sum=sum+j;
					System.out.print(j+" ");
				}
			}
			System.out.println();
		}
		
	}

}
