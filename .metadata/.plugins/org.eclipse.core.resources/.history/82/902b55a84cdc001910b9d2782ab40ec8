package eularProject;
import java.util.*;

public class NonAbundantSums {
	public static void main(String args[]) {
		ArrayList<Integer> al=new ArrayList<Integer>();
		for(int i=1;i<=28123;i++) {
			int sum=0;
			
			for(int j=1;j<i;j++) {
				
				if(i%j==0) {
					sum=sum+j;
					
				}
			}
			if(sum>i) {
				al.add(i);
			}
			
		}
		
	}

}
